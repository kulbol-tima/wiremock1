{
  "priority": 1,
  "request": {
    "method": "GET",
    "urlPath": "/api/internal/v1/get-pension-info",
    "queryParameters": {
      "pin": {
        "equalTo": "33333333333333"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json; charset=UTF-8"
    },
    "jsonBody": {
      "ok": "OK",
      "date": "{{now}}",
      "pin": "{{request.query.pin}}",
      "lastName": "Непенсионеров",
      "firstName": "Сидор",
      "patronymic": "Сидорович",
      "issueDate": "{{now}}",
      "pensionerInfo": []
    },
    "transformers": [
      "response-template"
    ]
  }
}
