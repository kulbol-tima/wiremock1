{
  "priority": 1,
  "request": {
    "method": "GET",
    "urlPath": "/api/internal/v1/get-pension-info-with-sum",
    "queryParameters": {
      "pin": {
        "equalTo": "11111111111111"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json; charset=UTF-8"
    },
    "jsonBody": {
      "date": "{{now}}",
      "state": "OK",
      "pin": "{{request.query.pin}}",
      "lastName": "Пенсионеров",
      "firstName": "Петр",
      "patronymic": "Иванович",
      "issueDate": "{{now}}",
      "pensionerInfo": [
        {
          "aop": "АОП1",
          "numDossier": "D123",
          "pinRecipient": "{{request.query.pin}}",
          "pinPayer": "123456789000",
          "dateTo": "2099-12-31",
          "kindPension": "По возрасту",
          "categoryPension": "По возрасту",
          "sum": "15000.00",
          "pin": "{{request.query.pin}}"
        }
      ]
    },
    "transformers": [
      "response-template"
    ]
  }
}
